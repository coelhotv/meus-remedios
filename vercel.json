{
  "version": 2,
  "functions": {
    "api/notify.js": { "maxDuration": 60 },
    "api/telegram.js": { "maxDuration": 10 },
    "api/dlq.js": { "maxDuration": 30 },
    "api/dlq/[id]/retry.js": { "maxDuration": 60 },
    "api/dlq/[id]/discard.js": { "maxDuration": 30 }
  },
  "routes": [
    {
      "src": "/api/dlq/([^/]+)/retry",
      "dest": "/api/dlq/$1/retry.js"
    },
    {
      "src": "/api/dlq/([^/]+)/discard",
      "dest": "/api/dlq/$1/discard.js"
    },
    {
      "src": "/api/telegram",
      "dest": "/api/telegram.js"
    },
    {
      "src": "/api/notify",
      "dest": "/api/notify.js"
    },
    {
      "src": "/api/dlq",
      "dest": "/api/dlq.js"
    },
    {
      "src": "/api/health/(.*)",
      "dest": "/api/health/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
