{
  "version": 2,
  "functions": {
    "api/notify.js": { "maxDuration": 60 },
    "api/telegram.js": { "maxDuration": 10 }
  },
  "rewrites": [
    {
      "source": "/api/telegram",
      "destination": "/api/telegram.js"
    },
    {
      "source": "/api/notify",
      "destination": "/api/notify.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}